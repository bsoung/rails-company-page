<h2><%= link_to portfolio_item.title, portfolio_show_path(portfolio_item) %></h2>
<p><%= portfolio_item.subtitle %></p>
<p><%= portfolio_item.body %></p>
<img src=<%= portfolio_item.thumb_image %>></img>
<%= link_to "Edit", edit_portfolio_path(portfolio_item) %>
<%= link_to 'Delete Portfolio Item', portfolio_show_path(portfolio_item), method: :delete, data: { confirm: 'Are you sure?' } %>